@model TherapyDashboard.Models.Patient

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <title>Radar chart</title>
    <script src="http://d3js.org/d3.v4.min.js"></script>
    <style>
        body {
            overflow: hidden;
            margin: 0;
            font-size: 14px;
            font-family: "Helvetica Neue", Helvetica;
        }

        .line {
            fill: none;
            stroke: steelblue;
            stroke-width: 2px;
        }

        #chart {
            position: absolute;
            top: 100px;
            left: 100px;
        }
        #line {
        	position: absolute;
        	top: 100px;
        	left: 800px;
        }
    </style>
</head>
<body>
    <div id="body">
        <div id="chart"></div>
        <div id="line"></div>
    </div>
    <script src="~/Scripts/Custom/DynamicSpider/D3Spider.js"></script>
    <script type="text/javascript" src="~/Scripts/Custom/DynamicSpider/SpiderConfig.js"></script>

    <script type="text/javascript" src="~/Scripts/Custom/DynamicSpider/InteractiveLine.js"></script>

</body>
</html>

